		</Part>
		-->
		
		<OverridePart Name="Audio">
			<Var Name="SteppedOn0">Wade0</Var>
			<Var Name="SteppedOn1">Wade1</Var>
			<Var Name="SteppedOn2">Wade2</Var>
		</OverridePart>
		
		<OverridePart Name="Render">
			<Var Name="Sprite">Empty</Var>
		</OverridePart>
		
	</Object>

	<Object Name="WaterFull" InheritsFrom="Liquid">

		<Part Name="Water">
			<Var Name="LiquidColor">#19affa</Var>
			<Var Name="Depth">6116</Var>
		</Part>

		<OverridePart Name="Render">
			<Var Name="DisplayName">Water</Var>
			<Var Name="DisplayNamePlural">Water</Var>
			<Var Name="Description">A pool of water. Probably not safe to drink as-is...</Var>
		</OverridePart>
		
	</Object>

	<Object Name="AcidFull" InheritsFrom="Liquid">

		<Part Name="Acid">
			<Var Name="LiquidColor">#BFFF00</Var>
			<Var Name="Depth">6116</Var>
		</Part>
		
		<OverridePart Name="Render">
			<Var Name="DisplayName">Acid</Var>
			<Var Name="DisplayNamePlural">Acid</Var>
			<Var Name="Description">A pool of roiling green acid. Extremely caustic. Strong acids aren't typically green, but you get a feeling your disbelief won't make this hazard any less potent.</Var>
		</OverridePart>
		
	</Object>

	<Object Name="BloodFull" InheritsFrom="Liquid">

		<Part Name="Blood">
			<Var Name="LiquidColor">#880808</Var>
			<Var Name="Depth">6116</Var>
		</Part>

		<OverridePart Name="Render">
			<Var Name="DisplayName">Blood</Var>
			<Var Name="DisplayNamePlural">Blood</Var>
			<Var Name="Description">A pool of blood.</Var>
		</OverridePart>

	</Object>

</Objects>
