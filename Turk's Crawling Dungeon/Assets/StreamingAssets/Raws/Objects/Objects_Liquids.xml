			<Var Name="UpperMiddle">LiquidUpperMiddle</Var>
			<Var Name="UpperRight">LiquidUpperRight</Var>
			<Var Name="CenterLeft">LiquidCenterLeft</Var>
			<Var Name="Center">Liquid</Var>
			<Var Name="CenterRight">LiquidCenterRight</Var>
			<Var Name="LowerLeft">LiquidLowerLeft</Var>
			<Var Name="LowerMiddle">LiquidLowerMiddle</Var>
			<Var Name="LowerRight">LiquidLowerRight</Var>
			<Var Name="Up">LiquidUp</Var>
			<Var Name="Down">LiquidDown</Var>
			<Var Name="Left">LiquidLeft</Var>
			<Var Name="Right">LiquidRight</Var>
